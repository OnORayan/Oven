{
  "name": "@ovenjs/core",
  "version": "0.1.0",
  "description": "Main Discord client for OvenJS that orchestrates REST and WebSocket",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },

      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "workspace:^",
    "@ovenjs/rest": "workspace:^",
    "@ovenjs/ws": "workspace:^"
  },
  "keywords": ["discord", "api", "client", "bot", "core"],
  "author": "OvenJS Team",
  "license": "MIT"
}