{
  "name": "@ovenjs/core",
  "version": "0.1.0",
  "description": "Main Discord client for OvenJS that orchestrates REST and WebSocket",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "^0.1.0",
    "@ovenjs/rest": "^0.1.0",
    "@ovenjs/ws": "^0.1.0"
  },
  "keywords": ["discord", "api", "client", "bot", "core"],
  "author": "OvenJS Team",
  "license": "MIT"
}