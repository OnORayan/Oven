{
  "name": "@ovenjs/rest",
  "version": "0.1.0",
  "description": "Discord REST API client with request handling.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },

      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc -noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "workspace:^",
    "undici": "^6.0.0"
  },
  "keywords": ["discord", "api", "rest", "client", "rate-limit"],
  "author": "OvenJS Team",
  "license": "MIT"
}