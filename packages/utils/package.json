{
  "name": "@ovenjs/utils",
  "version": "0.21.7",
  "description": "Utiliy package for ovenjs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "directories": {
    "lib": "./dist"
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "utils",
    "discord"
  ],
  "author": "OvenJS",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1"
}
