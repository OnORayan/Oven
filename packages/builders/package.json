{
  "name": "@ovenjs/builders",
  "version": "0.1.0",
  "description": "Type-safe builders for Discord objects in OvenJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "^0.1.0"
  },
  "keywords": ["discord", "api", "builders", "typescript", "type-safe"],
  "author": "OvenJS Team",
  "license": "MIT"
}