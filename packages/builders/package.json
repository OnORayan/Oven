{
  "name": "@ovenjs/builders",
  "version": "0.1.0",
  "description": "Type-safe builders for Discord objects in OvenJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },

      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "*"
  },
  "keywords": ["discord", "api", "builders", "typescript", "type-safe"],
  "author": "OvenJS Team",
  "license": "MIT"
}