{
  "name": "@ovenjs/ws",
  "version": "0.1.0",
  "description": "Discord WebSocket client with advanced connection management for OvenJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },

      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "*",
    "ws": "^8.16.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10"
  },
  "keywords": ["discord", "api", "websocket", "gateway", "sharding"],
  "author": "OvenJS Team",
  "license": "MIT"
}