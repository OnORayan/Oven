{
  "name": "@ovenjs/ws",
  "version": "0.1.0",
  "description": "Discord WebSocket client with advanced connection management for OvenJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovenjs/types": "^0.1.0",
    "ws": "^8.16.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10"
  },
  "keywords": ["discord", "api", "websocket", "gateway", "sharding"],
  "author": "OvenJS Team",
  "license": "MIT"
}